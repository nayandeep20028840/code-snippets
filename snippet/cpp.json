			"        if(t->r) t->r->par = t;",
			"    }",
			"    ///add operation",
			"    void push(pnode &t) {",
			"        if(!t)  return;",
			"        if (t->lazy_ans == 0 && t->lazy_val == 0) return;",
			"        t->ans += t->lazy_ans;",
			"        t->val += t->lazy_val;",
			"        t->mx += t->lazy_val;",
			"        t->mn += t->lazy_val;",
