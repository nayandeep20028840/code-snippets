			"    int rows() const {",
			"        return mat.size();",
			"    }",
			"",
			"    int cols() const {",
			"        return mat[0].size();",
			"    }",
			"",
			"    Matrix operator*(const Matrix<T>& obj) const {",
			"        assert(cols() == obj.rows());",
			"        vector<vector<T>> res(rows(), vector<T>(obj.cols()));",
			"        for(int r = 0; r < rows(); r++) {",
