			"    minHeap(int n) {",
			"        heap.resize(n) ;",
			"        siz = 0 ;",
			"        cap = n ;",
			"    }",
			"",
			"    void heapiFy(int i){",
			"        int smallest = i ;",
			"        int left = 2*i + 1 ;",
			"        int right = 2*i + 2 ;",
			"        if(left < siz && heap[left] < heap[smallest]){",
			"            smallest = left ;",
			"        }",
			"        if(right < siz && heap[right] < heap[smallest]){",
			"            smallest = right ;",
			"        }",
			"        if(smallest != i){",
			"            swap(heap[i] , heap[smallest]) ;",
			"            heapiFy(smallest) ;",
			"        }",
			"    }",
			"    // Implement the function to remove minimum element.",
			"    int extractMinElement() {",
