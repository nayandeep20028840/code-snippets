			"",
			"        // required according to the problem statement ",
			"        for (int i = 0; i < n; i++) {",
			"            for (int j = 0; j < n; j++) {",
			"                if (matrix[i][j] == 1e9) {",
			"                    matrix[i][j] = -1;",
			"                }",
			"            }",
			"        }",
			"    }",
			"};"
