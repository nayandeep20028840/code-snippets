		"    };",
		"    vector<node > dsu ; ",
		"",
		"    DSU(int n) { ",
		"        S = n ; ",
		"        for(int i = 0 ; i < n ; ++i) {",
		"            node tmp ; ",
		"            tmp.p = i; tmp.siz = 1; tmp.rnk = 0; ",
		"            dsu.push_back(tmp) ; ",
		"        }",
		"    }",
		"",
		"    void reset(int n) {",
		"        dsu.clear() ; ",
		"        S = n ; ",
		"        for(int i = 0 ; i < n ; ++i) {",
		"            node tmp ; ",
		"            tmp.p = i; tmp.siz = 1; tmp.rnk = 0; ",
		"            dsu.push_back(tmp) ; ",
		"        }",
		"    }",
		"",
		"    int parent(int u) {",
		"        if(dsu[u].p == u) return u;",
		"        dsu[u].p = parent(dsu[u].p) ; // Time Complexity O(log n)",
		"        return dsu[u].p ; ",
		"    }",
		"",
		"    void mergebysize(int u, int v) {",
        "        u = parent(u); v = parent(v);",
        "        if(u == v) return ;",
        "        if(dsu[u].siz < dsu[v].siz) swap(u , v) ; ",
        "        dsu[v].p = u ; ",
        "        dsu[u].siz += dsu[v].siz ; ",
        "    }",
        "",
        "    void mergebyrank(int u, int v) {",
        "        u = parent(u); v = parent(v);",
        "        if(u == v) return ;",
