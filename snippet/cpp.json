			"        if(t->r) {",
			"           t->r->lazy_ans += t->lazy_ans;",
			"           t->r->lazy_val += t->lazy_val;",
			"        }",
			"        t->lazy_ans = 0;",
			"        t->lazy_val = 0;",
			"    }",
			"    void reset(pnode &t) {",
			"        if(!t) return;",
			"        t->mx = t->val;",
			"        t->mn = t->val;",
			"    }",
			"    ///combine node l and r to form t by updating corresponding queries",
			"    void combine(pnode &t, pnode l, pnode r) {",
			"        if(!l) {",
			"            t = r; return;",
			"        }",
			"        if(!r) {",
			"            t = l; return;",
			"        }",
			"        t->mx = max(l->mx,r->mx);",
			"        t->mn = min(l->mn,r->mn);",
			"    }",
			"    ///perform all operations",
			"    void operation(pnode &t) {",
			"        if(!t)    return;",
			"        reset(t);",
