			"        if(siz == 0){",
			"            return -1 ;",
			"        }",
			"        int ans = heap[0] ;",
			"        swap(heap[0] , heap[siz - 1]) ;",
			"        siz-- ;",
			"        heapiFy(0) ;",
			"        return ans ;",
			"    }",
			"",
			"    // Implement the function to delete an element.",
			"    void deleteElement(int ind) {",
			"        // Write you code here.",
			"        if(ind >= siz){",
			"            return ;",
			"        }",
			"        swap(heap[ind] , heap[siz - 1]) ;",
			"        siz-- ;",
			"        heapiFy(ind) ;",
