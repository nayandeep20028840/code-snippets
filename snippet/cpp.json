			"            if (!used[i]) {",
			"                dfs1(i) ;",
			"            }",
			"        }",
			"",
			"        used.assign(n , false);",
			"        for (int i = 0 ; i < n ; ++i) {",
			"            int v = order[n - 1 - i] ;",
			"            if (!used[v]) {",
			"                dfs2(v) ;",
			"                components.push_back(component) ;",
			"                component.clear() ;",
			"            }",
			"        }",
			"",
			"        return components ;",
			"    }",
			"",
			"	bool visited(int v) {",
