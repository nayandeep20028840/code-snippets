			"struct Tarjan {",
			"    int n,dn,rt;",
			"    vector<vector<int>> g;",
			"    vector<vector<int>> vcc;",
			"    vector<int> dfn,low,cut;",
			"    stack<int> st;",
			"    Tarjan(int n):n(n),dn(0),g(n),dfn(n),low(n),cut(n) {}",
			"    void addEdge(int u,int v) {",
			"        g[u].emplace_back(v);",
			"        g[v].emplace_back(u);",
			"    }",
			"    void tarjan(int u) {",
			"        dfn[u]=low[u]=++dn;",
			"        st.push(u);",
			"        int chd=0;",
			"        for(int v:g[u]) {",
			"            if(!dfn[v]) {",
			"                tarjan(v);",
			"                low[u]=min(low[u], low[v]);",
			"                if(dfn[u]<=low[v]) {",
			"                    if(u!=rt||++chd>1) cut[u]=1;// ",
			"                    vcc.push_back({u});",
			"                    for (int x=-1; x!=v; st.pop())",
			"                        vcc.back().push_back(x=st.top());",
			"                }",
			"            }else {// ",
			"                // vcc",
			"                low[u]=min(low[u], dfn[v]);",
			"            }",
			"        }",
			"        if(u==rt&&chd==0)// ",
			"            vcc.push_back({u});",
			"    }",
			"    void converge() {",
			"        for(rt=0; rt<n; ++rt) ",
			"            if(!dfn[rt]) tarjan(rt);",
			"    }",
			"    // /",
			"    auto newTree() {",
			"        vector<vector<int>> tr(n+vcc.size());",
			"        for(int i=0; i<vcc.size(); ++i)",
			"            for(int x:vcc[i]) {",
			"                tr[n+i].push_back(x);// ",
			"                tr[x].push_back(n+i);",
			"            }",
			"        return tr;",
			"    }",
			"};"
